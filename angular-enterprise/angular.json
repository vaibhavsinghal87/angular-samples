{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "schematicCollections": ["angular-eslint"]
  },
  "newProjectRoot": "./",
  "projects": {
    "enterprise-shell": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "enterprise/enterprise-shell",
      "sourceRoot": "enterprise/enterprise-shell/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/enterprise-shell",
            "tsConfig": "enterprise/enterprise-shell/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "enterprise/enterprise-shell/public",
                "output": "."
              }
            ],
            "styles": [
              {
                "input": "node_modules/bootstrap/dist/css/bootstrap.min.css",
                "bundleName": "bootstrap"
              },
              {
                "input": "dist/core-ui/styles/theme.scss",
                "bundleName": "core-theme"
              },
              {
                "input": "enterprise/enterprise-shell/src/styles.scss",
                "bundleName": "enterprise-shell-styles"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["dist/core-ui/styles"]
            },
            "main": "enterprise/enterprise-shell/src/main.ts",
            "index": "enterprise\\enterprise-shell\\src\\index.html",
            "extraWebpackConfig": "enterprise/enterprise-shell/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "enterprise/enterprise-shell/webpack.prod.config.js"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "port": 4200,
            "publicHost": "http://localhost:4200",
            "extraWebpackConfig": "enterprise/enterprise-shell/webpack.config.js",
            "proxyConfig": "./proxy.conf.js"
          },
          "configurations": {
            "production": {
              "buildTarget": "enterprise-shell:build:production",
              "extraWebpackConfig": "enterprise/enterprise-shell/webpack.prod.config.js"
            },
            "development": {
              "buildTarget": "enterprise-shell:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "enterprise/enterprise-shell/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "enterprise/enterprise-shell/public"
              }
            ],
            "styles": [
              "enterprise/enterprise-shell/src/styles.scss"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "enterprise/enterprise-shell/**/*.ts",
              "enterprise/enterprise-shell/**/*.html"
            ],
            "eslintConfig": "enterprise/enterprise-shell/eslint.config.js"
          }
        }
      }
    },
